{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "react-native",
      "lib": [
        "es6"
      ],
      "moduleResolution": "node",
      "noEmit": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "strict": true
    },
    "exclude": [
      "node_modules"
    ],
    "extends": "expo/tsconfig.base"
  }
  